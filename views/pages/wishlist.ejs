<!DOCTYPE html>
<html>
<head>
  
</head>

<body>


<div class="container">
<h2>Your Wish List!</h2>

<ul>
    <% results.forEach(function(r) { %>
        <table>
        	<tr> 
        		<th>IMAGE</th>
        		<th>TITLE</th>
        		<th>AUTHOR</th>
        		<th>RATING</th> 
        	</tr>
        	<tr>
        		<td><img src="<%= r.pic%>" alt="<% =r.title%>"></td>
        		<td><%= r.title %> </td>
        		<td><%= r.author %> </td> 
        		<td><%= r.rate %> </td>
        	</tr>
        </table>

        <% }); %>
</ul>

</div>

</body>
</html>
