<!DOCTYPE html>
<html>
<head>
	<meta charset="utf-8">
    <title>Wishlist</title>
    <link rel="stylesheet" type="text/css" href="/stylesheets/main.css">
  
</head>

<body>


<div class="container">
<h2> <CENTER> Your Wishlist</CENTER></h2>
<form action = "/index">
          <input type="submit" class="ghost-button" value="Search for Another Title">
        </form>

<table id="wishlist">
	<tr>

		<th></th>
		<th>TITLE</th>
		<th>AUTHOR</th>
		<th>RATE</th>
		<th></th> 
	</tr>
	<% results.forEach(function(r) { %>
	<tr>
		<td> <img src=" <%= r.pic %>">
		<td><%= r.title %></td>
		<td><%= r.author %></td>
		<td><%= r. rate %></td>
		<td> remove</td>
	</tr>
	 <% }); %>


</table>

    
        
   

</div>

</body>
</html>
